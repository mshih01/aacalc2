{
  "name": "aacalc2",
  "version": "1.0.22",
  "description": "Axis and Allies probability calculator",
  "keywords": [
    "axis",
    "allies"
  ],
  "homepage": "https://github.com/mshih01/aacalc2",
  "bugs": {
    "url": "https://github.com/mshih01/aacalc2/issues"
  },
  "author": "Mitchell Shih <mitchell_shih@yahoo.com> (https://github.com/mshih01)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mshih01/aacalc2.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "license": "MIT",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.1",
    "@changesets/cli": "^2.29.4",
    "@types/node": "^22.15.29",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "ci": "npm run build && npm run check-format && npm run check-exports && npm run test",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "test": "vitest run",
    "dev": "vitest",
    "local-release": "changeset version && changeset publish",
    "prepublishOnly": "npm run ci"
  },
  "dependencies": {
    "heap-js": "^2.6.0"
  }
}
